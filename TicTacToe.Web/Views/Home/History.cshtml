@{
    ViewData["Title"] = "Games History";
}

@model TicTacToe.Common.Resources.GameResource[]

<table class="table">
    <thead>
        <tr>
            <th scope="col" style="width: 30%">#</th>
            <th scope="col" style="width: 30%">Result</th>
            <th scope="col" style="width: 30%">Moves</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Result</td>
                <td>@item.MovesHistory</td>
            </tr>    
        }
    </tbody>
</table>